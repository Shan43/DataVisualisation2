{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "International Visitors in Australia",
  "width": 800,
  "height": 400,
  "projection": {"type": "mercator"},
  "data": {
    "url": "https://raw.githubusercontent.com/slow0020/DataVisualisation2/refs/heads/main/STE_2021_AUST_GDA2020.json",
    "format": {"type": "topojson", "feature": "STE_2021_AUST_GDA2020"}
  },
  "transform": [
    {
      "lookup": "properties.STE_NAME21",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/slow0020/DataVisualisation2/refs/heads/main/data/CumulativeVisitorArrivalData.csv"
        },
        "key": "State",
        "fields": ["Visitor "]
      }
    }
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "Visitor ",
      "type": "quantitative",
      "scale": {"type": "log"}
    },
    "tooltip": [
      {"field": "properties.STE_NAME21", "type": "nominal", "title": "State"},
      {"field": "Visitor ", "type": "quantitative"}
    ]
  },
  "config": {}
}