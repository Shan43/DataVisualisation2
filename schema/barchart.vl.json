{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "placeholder",
  "width": 500,
  "height": 400,
  "projection": {"type": "mercator"},
  "data": {
    "url": "https://raw.githubusercontent.com/slow0020/DataVisualisation2/refs/heads/main/data/AustraliaVisitorArrivalData.csv"
  },
  "params": [
    {
      "name": "State_selection",
      "bind": {
        "input": "select",
        "options": [
          null,
          "Australian Capital Territory",
          "New South Wales",
          "Northern Territory",
          "Queensland",
          "South Australia",
          "Tasmania",
          "Victoria",
          "Western Australia"
        ],
        "labels": [
          "Show All",
          "Australian Capital Territory",
          "New South Wales",
          "Northern Territory",
          "Queensland",
          "South Australia",
          "Tasmania",
          "Victoria",
          "Western Australia"
        ],
        "name": "State Selection: "
      }
    }
  ],
  "mark": {"type": "bar"},
  "encoding": {
    "x": {
      "field": "Visitor Arrival",
      "type": "quantitative",
      "title": "Short-Term Visitors in Australia"
    },
    "y": {"field": "State", "type": "nominal", "title": "State/Territory"},
    "color": {"field": "State", "type": "nominal"},
    "tooltip": [
      {"field": "State", "type": "nominal"},
      {"field": "Visitor Arrival", "type": "quantitative"},
      {"field": "Year", "type": "quantitative"}
    ]
  },
  "config": {}
}